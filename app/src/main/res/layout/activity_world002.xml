    <Button
        style="@style/world001endturnbtn"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="End Turn"
        android:id="@+id/bEndTurn"
        android:layout_marginBottom="5dp"
        android:enabled="true" />

    <!-- PLAYER PORTRAIT -->
    <ImageView
        android:contentDescription="@null"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:id="@+id/ivPlayerPortrait"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/portrait_placeholder_boy"
        android:scaleType="fitCenter"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/frame_black" />

    <!-- ENEMY PORTRAIT -->
    <ImageView
        android:contentDescription="@null"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:id="@+id/ivEnemyPortrait"
        android:src="@drawable/portrait_dungeon"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:scaleType="fitCenter"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/frame_black" />

    <!-- PLAYER NAME -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PLAYER NAME"
        android:id="@+id/tvPlayerName"
        android:layout_below="@+id/ivPlayerPortrait"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/ivPlayerPortrait"
        android:textStyle="bold"
        android:textColor="@color/textWhite" />

    <!-- PLAYER LEVEL TITLE -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPlayerName"
        android:layout_toRightOf="@+id/ivPlayerPortrait"
        android:text="Lv: "
        android:id="@+id/tvLvTitle"
        android:textStyle="bold"
        android:textColor="@color/textWhite" />

    <!-- PLAYER LEVEL NUMBER -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPlayerName"
        android:layout_toRightOf="@+id/tvLvTitle"
        android:text="1"
        android:id="@+id/tvLvNumber"
        android:textStyle="bold"
        android:textColor="@color/textWhite"
        android:layout_marginRight="5dp" />

    <!-- PLAYER EXP TITLE -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPlayerName"
        android:layout_toRightOf="@+id/tvLvNumber"
        android:text="Exp: "
        android:id="@+id/tvExpTitle"
        android:textStyle="bold"
        android:textColor="@color/textWhite" />

    <!-- PLAYER EXP NUMBER -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPlayerName"
        android:layout_toRightOf="@+id/tvExpTitle"
        android:text="0 "
        android:id="@+id/tvExpNumber"
        android:textStyle="bold"
        android:textColor="@color/textWhite" />

    <!-- PLAYER EXP OF HUNDRED -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPlayerName"
        android:layout_toRightOf="@+id/tvExpNumber"
