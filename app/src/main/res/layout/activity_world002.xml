        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvExpTitle"
        android:layout_toRightOf="@+id/tvPlayerMovesTitle"
        android:text="3"
        android:id="@+id/tvPlayerMovesLeftNumber"
        android:textStyle="bold"
        android:textColor="@color/textWhite" />

    <!-- ENEMY NAME -->
