        android:layout_height="wrap_content"
        android:layout_below="@+id/tvExpTitle"
        android:layout_toRightOf="@+id/ivPlayerPortrait"
        android:text="Moves: "
        android:id="@+id/tvPlayerMovesTitle"
        android:textStyle="bold"
        android:textColor="@color/textWhite" />

    <!-- REMOVALS LEFT NUMBER -->
