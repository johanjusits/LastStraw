<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/bg_world002"
    android:id="@+id/rootLayout">

    <!-- PLAYER STATUS ICONS-->
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/buff_concentrate"
        android:id="@+id/tvPlayerStatus1"
        android:layout_below="@+id/ivPlayerPortrait"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="5dp"
        android:visibility="invisible"
        android:background="@drawable/frame_black" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/debuff_slow"
        android:id="@+id/tvPlayerStatus2"
        android:layout_toRightOf="@+id/tvPlayerStatus1"
        android:layout_below="@+id/ivPlayerPortrait"
        android:background="@drawable/frame_white"
        android:layout_marginBottom="10dp"
        android:visibility="invisible"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/card_not_available"
        android:id="@+id/tvPlayerStatus3"
        android:layout_toRightOf="@+id/tvPlayerStatus2"
        android:layout_below="@+id/ivPlayerPortrait"
        android:background="@drawable/frame_black"
        android:layout_marginBottom="10dp"
        android:visibility="invisible"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/card_not_available"
        android:id="@+id/tvPlayerStatus4"
        android:layout_toRightOf="@+id/tvPlayerStatus3"
        android:layout_below="@+id/ivPlayerPortrait"
        android:background="@drawable/frame_black"
        android:layout_marginBottom="10dp"
        android:visibility="invisible"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/card_not_available"
        android:id="@+id/tvPlayerStatus5"
        android:layout_toRightOf="@+id/tvPlayerStatus4"
        android:layout_below="@+id/ivPlayerPortrait"
        android:background="@drawable/frame_black"
        android:layout_marginBottom="10dp"
        android:visibility="invisible"/>

    <!-- ENEMY STATUS ICONS -->
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/card_not_available"
        android:id="@+id/tvEnemyStatus1"
        android:layout_below="@+id/ivEnemyPortrait"
